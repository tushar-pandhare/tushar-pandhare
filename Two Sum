class Solution {
public:
    vector<int> twoSum(vector<int>& a, int target) {

        for(int i=0;i<=a.size()-1;i++){
            for(int j=i+1;j<=a.size()-1;j++)
            {
              if(a[i]+ a[j] == target){
              return {i, j};
              }
            }
        }
        return {};
    }
};
